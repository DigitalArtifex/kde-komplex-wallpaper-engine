<?xml version="1.0" encoding="UTF-8"?>
<kcfg xmlns="http://www.kde.org/standards/kcfg/1.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.kde.org/standards/kcfg/1.0/kcfg.xsd" >
  <kcfgfile name=""/>

  <group name="General">
    <entry name="selectedShaderPath" type="String">
      <label>SelectedShader</label>
      <default>Shaders6/Ps3menu.frag.qsb</default>
    </entry>
    <entry name="selectedShaderContent" type="String">
      <label>selectedShaderContent</label>
      <default></default>
    </entry>
    <entry name="selectedShaderIndex" type="Int">
      <label>The index of the currently selectedShader</label>
      <default>0</default>
    </entry>
    <entry name="mouseAllowed" type="Bool">
      <label>Allow use of mouse in shader</label>
      <default>false</default>
    </entry>
    <entry name="mouseSpeedBias" type="Double">
      <label>Change the mouse's bias speed</label>
      <default>1.0</default>
    </entry>
    <entry name="running" type="Bool">
      <label>Shader is running.</label>
      <default>true</default>
    </entry>
    <entry name="checkedSmartPlay" type="Bool">
      <label>Pause the video when existing a maximized or full-screen windows.</label>
      <default>false</default>
    </entry>
    <entry name="shaderSpeed" type="Double">
      <label>Change the shader speed</label>
      <default>1.0</default>
    </entry>
    <entry name="iChannel0" type="String">
      <label>iChannel0's path</label>
      <default>./Resources/wallpaper2.png</default>
    </entry>
    <entry name="iChannel0_type" type="Int">
      <label>iChannel0's Channel Type</label>
      <default>0</default>
    </entry>
    <entry name="iChannel0_resolution_x" type="Int">
      <label>iChannel0's Resolution (Width)</label>
      <default>1920</default>
    </entry>
    <entry name="iChannel0_resolution_y" type="Int">
      <label>iChannel0's Resolution (Height)</label>
      <default>1080</default>
    </entry>
    <entry name="iChannel0_resolution_scale" type="Double">
      <label>iChannel0's Resolution (Scale)</label>
      <default>1</default>
    </entry>
    <entry name="iChannel0_flag" type="Bool">
      <label>iChannel0 on/off flag</label>
      <default>true</default>
    </entry>
    <entry name="iChannel0_inverted" type="Bool">
      <label>Invert iChannel0's data for the shader</label>
      <default>true</default>
    </entry>
    <entry name="iChannel0_timeScale" type="Double">
      <label>iChannel0 time scale</label>
      <default>1.0</default>
    </entry>
    <entry name="iChannel1" type="String">
      <label>iChannel1's path</label>
      <default>./Resources/wallpaper2.png</default>
    </entry>
    <entry name="iChannel1_type" type="Int">
      <label>iChannel1's Channel Type</label>
      <default>0</default>
    </entry>
    <entry name="iChannel1_resolution_x" type="Int">
      <label>iChannel1's Resolution (Width)</label>
      <default>1920</default>
    </entry>
    <entry name="iChannel1_resolution_y" type="Int">
      <label>iChannel1's Resolution (Height)</label>
      <default>1080</default>
    </entry>
    <entry name="iChannel1_resolution_scale" type="Double">
      <label>iChannel1's Resolution (Scale)</label>
      <default>1</default>
    </entry>
    <entry name="iChannel1_flag" type="Bool">
      <label>iChannel1 on/off flag</label>
      <default>true</default>
    </entry>
    <entry name="iChannel1_inverted" type="Bool">
      <label>Invert iChannel1's data for the shader</label>
      <default>true</default>
    </entry>
    <entry name="iChannel1_timeScale" type="Double">
      <label>iChannel1 time scale</label>
      <default>1.0</default>
    </entry>
    <entry name="iChannel2" type="String">
      <label>iChannel2's path</label>
      <default>./Resources/Shadertoy_Organic_2.jpg</default>
    </entry>
    <entry name="iChannel2_type" type="Int">
      <label>iChannel2's Channel Type</label>
      <default>0</default>
    </entry>
    <entry name="iChannel2_resolution_x" type="Int">
      <label>iChannel2's Resolution (Width)</label>
      <default>1920</default>
    </entry>
    <entry name="iChannel2_resolution_y" type="Int">
      <label>iChannel2's Resolution (Height)</label>
      <default>1080</default>
    </entry>
    <entry name="iChannel2_resolution_scale" type="Double">
      <label>iChannel2's Resolution (Scale)</label>
      <default>1</default>
    </entry>
    <entry name="iChannel2_flag" type="Bool">
      <label>iChannel2 on/off flag</label>
      <default>true</default>
    </entry>
    <entry name="iChannel2_inverted" type="Bool">
      <label>Invert iChannel2's data for the shader</label>
      <default>true</default>
    </entry>
    <entry name="iChannel2_timeScale" type="Double">
      <label>iChannel2 time scale</label>
      <default>1.0</default>
    </entry>
    <entry name="iChannel3" type="String">
      <label>iChannel3's path</label>
      <default>./Resources/Shadertoy_Organic_2.jpg</default>
    </entry>
    <entry name="iChannel3_type" type="Int">
      <label>iChannel3's Channel Type</label>
      <default>0</default>
    </entry>
    <entry name="iChannel3_resolution_x" type="Int">
      <label>iChannel3's Resolution (Width)</label>
      <default>1920</default>
    </entry>
    <entry name="iChannel3_resolution_y" type="Int">
      <label>iChannel3's Resolution (Height)</label>
      <default>1080</default>
    </entry>
    <entry name="iChannel3_resolution_scale" type="Double">
      <label>iChannel3's Resolution (Scale)</label>
      <default>1</default>
    </entry>
    <entry name="iChannel3_flag" type="Bool">
      <label>iChannel3 on/off flag</label>
      <default>false</default>
    </entry>
    <entry name="iChannel3_inverted" type="Bool">
      <label>Invert iChannel3's data for the shader</label>
      <default>true</default>
    </entry>
    <entry name="iChannel3_timeScale" type="Double">
      <label>iChannel3 time scale</label>
      <default>1.0</default>
    </entry>
    <entry name="resolution_x" type="Int">
      <label>Output Channel's Resolution (Width)</label>
      <default>1920</default>
    </entry>
    <entry name="resolution_y" type="Int">
      <label>Output Channel's Resolution (Height)</label>
      <default>1080</default>
    </entry>
    <entry name="resolution_scale" type="Double">
      <label>Output resolution scale</label>
      <default>1.0</default>
    </entry>
    <entry name="framerate_limit" type="Double">
      <label>Frame Rate Limit</label>
      <default>60</default>
    </entry>
    <entry name="shader_package" type="String">
      <label>Shader Package</label>
      <default></default>
    </entry>
    <entry name="shader_package_index" type="Int">
      <label>Shader Package Index</label>
      <default>-1</default>
    </entry>
    <entry name="komplex_mode" type="Int">
      <label>Use the Komplex Mode</label>
      <default>0</default>
    </entry>
    <entry name="fadeToNext" type="Bool">
      <label>fade to next shader in folder</label>
      <default>false</default>
    </entry>
    <entry name="fadeToRandom" type="Bool">
      <label>fade to random shader in folder</label>
      <default>false</default>
    </entry>
    <entry name="infoPlasma6Preview_dismissed" type="Bool">
      <label>Hide the infoPlasma6Preview message</label>
      <default>false</default>
    </entry>
    <entry name="warningResources_dismissed" type="Bool">
      <label>Hide the warningResources message</label>
      <default>false</default>
    </entry>
    <entry name="emergencyHelp_dismissed" type="Bool">
      <label>Hide the emergencyHelp message</label>
      <default>false</default>
    </entry>
    <entry name="infoiChannelSettings_dismissed" type="Bool">
      <label>Hide the infoiChannelSettings message</label>
      <default>false</default>
    </entry>
    <entry name="pauseMode" type="Int">
      <label>Shader pause mode
        0: when existing a maximized or full-screen windows
        1: when a visible window is active
        2: when a window is visible
        3: never</label>
      <default>0</default>
    </entry>
    <entry name="checkActiveScreen" type="Bool">
      <label>Whether or not check for windows from all screens instead of just the active one</label>
      <default>true</default>
    </entry>
    <entry name="excludeWindows" type="StringList">
      <label>A list of app-IDs whose windows to exclude from triggering pauseMode.</label>
      <default></default>
    </entry>
  </group>

</kcfg>
